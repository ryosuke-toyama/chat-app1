<div class="side-header">
  <div class="user-name">
  <%= link_to edit_user_path(current_user) do %>
    <%= current_user.name%>
    <% end %>
  </div>
  <div class="chat-edit"><%= link_to "チャットを作成する", new_room_path %></div>
</div>
<div class="side-main">
    <% current_user.rooms.each do |room| %>
    <div class="chat-room">
      <div class="chat-name">
        <a href="#"><%= room.name%></a>
      </div>
    </div>
  <% end %>
</div>